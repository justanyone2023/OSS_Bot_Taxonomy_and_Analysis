@article{SPIE199393,
title = {Characterization of the chloride carrier in the plasmalemma of the alga Valonia utricularis: the inhibition by 4,4′-diisothiocyanatostilbene-2,2′-disulfonic acid},
journal = {Biochimica et Biophysica Acta (BBA) - Biomembranes},
volume = {1149},
